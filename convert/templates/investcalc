  <script>
    function M_P_N() {
      var TDs = document.querySelectorAll('.plusmin');
    
      for (var i = 0; i < TDs.length; i++) {
        var temp = TDs[i];
        if (temp.firstChild.nodeValue.indexOf('-') == 0) {temp.className = "negative";}
        else {temp.className = "positive";}
      }
    }
      function countvRpe(){
        var v_rpe_highest_pe = parseFloat(document.getElementById("rpe_highest_pe").value);
        var v_rpe_lowest_pe = parseFloat(document.getElementById("rpe_lowest_pe").value);
        var v_rpe_diluted_eps = parseFloat(document.getElementById("rpe_diluted_eps").value);
        var v_rpe_share_price = parseFloat(document.getElementById("rpe_share_price").value);
        var v_rpe_rpe = (v_rpe_highest_pe+v_rpe_lowest_pe)/2;
        var v_rpe_intrinsic_value =  v_rpe_rpe*v_rpe_diluted_eps;
        var v_rpe_margin_safety =  ((v_rpe_intrinsic_value-v_rpe_share_price)/v_rpe_intrinsic_value)*100;
        var s_rpe_margin_safety = document.getElementById("rpe_margin_safety");
        var s_rpe_intrinsic_value = document.getElementById('rpe_intrinsic_value');
        var s_rpe_avg_pe = document.getElementById('rpe_avg_pe');
        s_rpe_intrinsic_value.innerHTML = "$"+(v_rpe_intrinsic_value.toFixed(2));
        s_rpe_avg_pe.innerHTML = v_rpe_rpe.toFixed(2);
        s_rpe_margin_safety.innerHTML = v_rpe_margin_safety.toFixed(2)+"%";
        M_P_N();
      }
  </script>
